/* 导航栏容器 */
.navigation-wrapper{
  padding: 0 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

/* 导航栏按钮 */
.navigation-btn{
  display: flex;
  align-items: center;
  height: 28px;
  padding: 0 10rpx;
  border-radius: 10px;
  border: 1px solid #999;
}

/* 图标 */
.back,.home{
  width: 18px;
  height: 18px;
}

.back{
  margin-right: 20rpx;
}

image{
  width: 100%;
  height: 100%;
}

.hide{
  visibility: hidden;
}